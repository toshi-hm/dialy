{
  "name": "dialy",
  "version": "0.1.0",
  "private": true,
  "description": "A diary app that lets you see past entries for the same day at a glance",
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
